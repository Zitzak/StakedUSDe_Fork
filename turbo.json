{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "out/**",
        "artifacts/**",
        "cache/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "deploy:staked-piku": {
      "inputs": [".env"],
      "env": ["PRIVATE_KEY", "SEPOLIA_RPC_URL", "ETHERSCAN_API_KEY"],
      "dependsOn": [
        "build",
        "test"
      ],
      "outputs": [
        "deployments/**"
      ]
    },
    "deploy:merkle-drop": {
      "inputs": [".env"],
      "env": ["PRIVATE_KEY", "SEPOLIA_RPC_URL", "ETHERSCAN_API_KEY"],
      "dependsOn": [
        "build",
        "test"
      ]
    },
    "deploy:merkle-drop:withTransferOwner": {
      "inputs": [".env"],
      "env": ["PRIVATE_KEY", "MAINNET_RPC_URL", "ETHERSCAN_API_KEY"],
      "dependsOn": [
        "build",
        "test"
      ]
    },
    "clean": {
    },
    "generate-abis": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "packages/shared-abi/abis/**"
      ],
      "cache": false
    }
  }
}
